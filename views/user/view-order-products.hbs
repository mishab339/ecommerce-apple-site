<section>
    <div class="container mt-5">
         <div class="row">
            {{#each products}}
             <div class="col-md-4 p-3 d-flex justify-content-center">
                <div class="card cb1 text-center oredr-product-card">
                    <img src="/product-images/{{this.product._id}}.png" class="card-img-top" alt="...">
                    <div class="card-body">
                        <h5 class="card-title">{{this.Name}}</h5>
                        <p class="card-text">Rs.{{this.product.Price}}</p>
                        <p class="card-text">Quantity : {{this.quantity}}</p>
                        <p class="card-text">{{this.Discription}}</p>
                    </div>
                </div>
            </div>
            {{/each}}
         </div>
    </div>
</section>